{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "clean": "rm -rf ./amplify/backend/function/PackageAnalyzer/src/dist",
    "copy": "cp -r ./dist ./amplify/backend/function/PackageAnalyzer/src/dist",
    "deploy": "yarn run clean; yarn build; yarn run copy; amplify push --yes"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {},
  "name": "package-analyzer-server",
  "author": "Herman Starikov",
  "module": "dist/package-analyzer.esm.js",
  "size-limit": [
    {
      "path": "dist/package-analyzer.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/package-analyzer.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.7.0",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.8",
    "husky": "^4.3.0",
    "size-limit": "^4.7.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@types/uuid": "^8.3.0",
    "aws-sdk": "^2.784.0",
    "aws-serverless-express": "^3.3.8",
    "uuid": "^8.3.1"
  }
}
